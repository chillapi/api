{
  "name": "@chillapi/api",
  "version": "1.0.21",
  "description": "ChillAPI interface package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate-openapi": "rimraf src/generated && node generate-openapi.mjs",
    "prebuild": "rimraf dist/* && npm run generate-openapi",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm run lint && npm run tsc",
    "build:watch": "tsc -w",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chillapi/api.git"
  },
  "keywords": [
    "ChillAPI",
    "API",
    "OpenAPI"
  ],
  "author": "Valentin Raduti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chillapi/api/issues"
  },
  "homepage": "https://github.com/chillapi/api#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/node": "^14.0.27",
    "@types/jest": "^26.0.23",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.2",
    "typescript": "^4.0.5",
    "jest": "^27.0.5",
    "rimraf": "^3.0.2",
    "json-schema-to-typescript": "^10.1.4"
  }
